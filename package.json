{
  "name": "@moda/om",
  "version": "18.4.0",
  "description": "Moda Operandi design system",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/src/index.d.ts",
  "sass": "src/index.scss",
  "style": "dist/styles.css",
  "repository": "git@github.com:ModaOperandi/om.git",
  "author": "ModaOperandi",
  "license": "MIT",
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "scripts": {
    "build:documentation": "NODE_ENV=production cd documentation && yarn build",
    "build:storybook": "NODE_ENV=storybook storybook build",
    "build": "NODE_ENV=production yarn compile && yarn types",
    "clean": "rm -rf ./dist",
    "commit": "yarn git-cz",
    "compile": "yarn rollup -c --bundleConfigAsCjs",
    "lint": "eslint src --ext .tsx,.ts --fix",
    "format": "prettier --write '**/*'",
    "predeploy:documentation": "NODE_ENV=production yarn build && cd documentation && yarn install && yarn build",
    "prepublishOnly": "yarn clean && yarn build && pinst --disable",
    "scaffold:component": "node scripts/scaffold/react Components",
    "scaffold:mixin": "node scripts/scaffold/sass Mixins",
    "scaffold:function": "node scripts/scaffold/sass Functions",
    "semantic-release": "semantic-release",
    "start": "cd documentation && yarn start",
    "storybook": "storybook dev -p 6006",
    "test": "jest -w 1",
    "types": "tsc -p tsconfig.json --emitDeclarationOnly",
    "unlink-all": "yalc remove --all && yarn --check-files",
    "postinstall": "husky install",
    "postpublish": "pinst --enable"
  },
  "peerDependencies": {
    "react": ">=18.1.0",
    "react-dom": ">=18.1.0",
    "react-router-dom": "^6.19.0"
  },
  "dependencies": {
    "@moda/icons": "^5.18.0",
    "@moda/tokens": "^5.8.0",
    "@storybook/addon-styling": "^1.0.1",
    "@types/classnames": "^2.2.9",
    "classnames": "^2.2.6",
    "credit-card-type": "^10.0.0",
    "focus-visible": "^5.0.2",
    "proportional-scale": "^4.0.0",
    "react-focus-on": "^3.3.0",
    "react-input-mask": "^2.0.4",
    "scroll-into-view-if-needed": "^3.0.3",
    "storybook": "^7.0.5",
    "use-cursor": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.0",
    "@babel/preset-typescript": "^7.9.0",
    "@commitlint/cli": "^18.0.0",
    "@commitlint/config-conventional": "^18.0.0",
    "@jedmao/semantic-release-npm-github-config": "^1.0.9",
    "@moda/rollup-plugin-postcss": "^4.0.1",
    "@rollup/plugin-babel": "^6.0.0",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@storybook/addon-actions": "^7.0.2",
    "@storybook/addon-essentials": "^7.0.2",
    "@storybook/addon-links": "^7.0.2",
    "@storybook/addon-mdx-gfm": "^7.0.5",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-viewport": "^7.0.2",
    "@storybook/addons": "^7.0.2",
    "@storybook/react": "^7.0.2",
    "@storybook/react-webpack5": "^7.0.5",
    "@testing-library/jest-dom": "^6.1.2",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.1.1",
    "@types/cheerio": "^0.22.22",
    "@types/credit-card-type": "^9.0.0",
    "@types/jest": "^29.0.3",
    "@types/ramda": "^0.29.0",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.1",
    "@types/react-input-mask": "^3.0.2",
    "@types/react-router-dom": "^5.1.4",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "autoprefixer": "^10.0.4",
    "babel-loader": "^9.0.0",
    "commitizen": "^4.0.3",
    "css-loader": "^6.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-prettier-react": "^0.0.24",
    "eslint-config-standard-jsx": "^11.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.16.0",
