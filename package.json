{
  "name": "@moda/om",
  "version": "15.10.3",
  "description": "Moda Operandi design system",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "sass": "src/index.scss",
  "style": "dist/styles.css",
  "repository": "git@github.com:ModaOperandi/om.git",
  "author": "ModaOperandi",
  "license": "MIT",
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "scripts": {
    "build:documentation": "NODE_ENV=production cd documentation && yarn build",
    "build:storybook": "NODE_ENV=storybook build-storybook",
    "build": "NODE_ENV=production yarn compile && yarn types",
    "clean": "rm -rf ./dist",
    "commit": "yarn git-cz",
    "compile": "yarn rollup -c",
    "lint": "eslint src --ext .tsx,.ts --fix",
    "format": "prettier --write '**/*'",
    "predeploy:documentation": "NODE_ENV=production yarn build && cd documentation && yarn install && yarn build",
    "prepublishOnly": "yarn clean && yarn build && pinst --disable",
    "scaffold:component": "node scripts/scaffold/react Components",
    "scaffold:mixin": "node scripts/scaffold/sass Mixins",
    "scaffold:function": "node scripts/scaffold/sass Functions",
    "semantic-release": "semantic-release",
    "start": "cd documentation && yarn start",
    "storybook": "start-storybook -p 6006",
    "test": "jest -w 1",
    "types": "tsc -p tsconfig.json --emitDeclarationOnly",
    "unlink-all": "yalc remove --all && yarn --check-files",
    "postinstall": "husky install",
    "postpublish": "pinst --enable"
  },
  "peerDependencies": {
    "react": ">=17.0.1",
    "react-dom": ">=17.0.1",
    "react-router-dom": "^5.1.2"
  },
  "dependencies": {
    "@moda/icons": "^5.14.0",
    "@moda/tokens": "^5.5.1",
    "@types/classnames": "^2.2.9",
    "classnames": "^2.2.6",
    "credit-card-type": "^9.0.0",
    "focus-visible": "^5.0.2",
    "proportional-scale": "^4.0.0",
    "react-focus-on": "^3.3.0",
    "react-input-mask": "^2.0.4",
    "scroll-into-view-if-needed": "^2.2.26",
    "use-cursor": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.0",
    "@babel/preset-typescript": "^7.9.0",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@jedmao/semantic-release-npm-github-config": "^1.0.9",
    "@moda/rollup-plugin-postcss": "^4.0.1",
    "@rollup/plugin-babel": "^5.2.3",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@storybook/addon-actions": "^6.2.1",
    "@storybook/addon-essentials": "^6.2.1",
    "@storybook/addon-links": "^6.2.1",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-viewport": "^6.0.26",
    "@storybook/addons": "^6.0.26",
    "@storybook/builder-webpack5": "^6.2.1",
    "@storybook/manager-webpack5": "^6.3.1",
    "@storybook/react": "^6.2.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.0",
    "@testing-library/user-event": "^13.0.7",
    "@types/cheerio": "^0.22.22",
    "@types/credit-card-type": "^9.0.0",
    "@types/enzyme": "^3.10.3",
    "@types/jest": "^27.0.0",
    "@types/ramda": "^0.27.40",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.10",
    "@types/react-input-mask": "^2.0.5",
    "@types/react-router-dom": "^5.1.4",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.0",
    "autoprefixer": "^10.0.4",
    "babel-loader": "^8.1.0",
    "commitizen": "^4.0.3",
    "css-loader": "^6.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "enzyme": "^3.10.0",
    "eslint": "^8.0.1",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.0",
    "identity-obj-proxy": "^3.0.0",
    "ignore-not-found-export-webpack-plugin": "^1.0.1",
    "jest": "^27.3.1",
    "lint-staged": "^12.0.2",
    "node-sass": "^7.0.0",
    "node-sass-package-importer": "^5.3.2",
    "pinst": "^2.1.4",
    "postcss": "^8.1.10",
    "postcss-loader": "^6.1.0",
    "prettier": "^2.0.1",
    "ramda": "^0.27.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.1.2",
    "rollup": "^2.38.5",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-scss": "^3.0.0",
    "sass-loader": "^12.0.0",
    "semantic-release": "^18.0.0",
    "storybook-states": "^1.2.0",
    "style-loader": "^3.0.0",
    "ts-jest": "^27.0.7",
    "typescript": "^4.0.2",
    "waait": "^1.0.5",
    "webpack": "^5.5.1",
    "webpack-cli": "^4.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
